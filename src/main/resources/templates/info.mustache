<script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
<script>
    async function parseData(data){
        return new Promise(resolve => {
            resolve(JSON.parse(data))
        })
    }
    $(document).ready(function(){
        $.ajax({
            url:'http://localhost:8080/parkinglot_info?' +
                    "latitude=" + localStorage.getItem("latitude") +
                    "&longitude=" + localStorage.getItem("longitude"),
            type:'GET',
            dataType:'text',
            beforeSend:function(jqXHR) {
                console.log("requesting ajax..");
            },
            success: function(data) {
                document.getElementById("time").innerHTML = JSON.parse(data).response.body.items[0]["operDay"];
                document.getElementById("pos").innerHTML = JSON.parse(data).response.body.items[0]["lnmadr"];
                document.getElementById("admin_number").innerHTML = JSON.parse(data).response.body.items[0]["phoneNumber"];
                console.log(JSON.parse(data));
                $("#Area").html(data);
            },
            error:function(jqXHR) {
                console.log("request failed");
            }
        });
        $("#using").click(function(){
            $.ajax({
                url:'http://localhost:8080/want',
                //url:'http://ec2-3-35-94-87.ap-northeast-2.compute.amazonaws.com:8080/want',
                type:'POST',
                //contentType : 보내는 데이터 형식
                //dataType : 받는 데이터 형식
                contentType:'application/json',
                data: JSON.stringify({"value":"1"}),
                success: function(){
                    console.log("request success");
                    window.location.href = '/using';

                },
                error:function(jqXHR) {
                    console.log("request failed");
                }
            });
        });
    });
